  "devdoc": {
    "details": "Token router to swap between the base token and one of serveral underlying stablecoins that the protocol is built on",
    "events": {
      "TokensBought(address,address,uint256[3],uint256,uint256)": {
        "details": "Emitted when base tokens are purchased on Uniswap",
        "params": {
          "assetTokenAmount": "The amount of asset tokens being spent",
          "baseTokenReturned": "The amount of base tokens received",
          "recipient": "The address of the recipient of the base token",
          "sender": "The sender of the underlying stablecoins",
          "stableAmounts": "The amounts of stablecoins to spend                      (0 = DAI, 1 = USDC, 2 = USDT)"
        }
      },
      "TokensSoldForAsset(address,address,uint256,uint256)": {
        "details": "Emitted when base tokens are sold on Uniswap for asset tokens",
        "params": {
          "assetTokensReturned": "The amount of asset tokens returned to the                            recipient Note that due to dust, assetTokensReturned may be more than assetTokenAmount.",
          "baseTokenAmount": "The amount of base tokens spent",
          "recipient": "The address of the recipient of the underlying stablecoins",
          "sender": "The sender of the base token"
        }
      },
      "TokensSoldForStable(address,address,uint256,uint256,uint256,uint256)": {
        "details": "Emitted when base tokens are sold on Uniswap for stablecoins",
        "params": {
          "assetTokensUnstaked": "The amount of asset tokens unstaked from the                            Curve Aave pool",
          "baseTokenSpent": "The amount of base tokens spent",
          "recipient": "The address of the recipient of the underlying stablecoins",
          "sender": "The sender of the base token",
          "stableIndex": "The index of the stablecoin to receive                    (0 = DAI, 1 = USDC, 2 = USDT)",
          "stablesReturned": "The amount of stablecoins returned to the recipient Note that due to dust, stablesReturned may be more than stablesUnstaked."
        }
      }
    },
    "kind": "dev",
    "methods": {
      "buyTokens(uint256[3],uint256,address)": {
        "details": "See {IUniV3Swapper-buyTokens}"
      },
      "buyTokensOneStable(uint256,uint256)": {
        "details": "See {IUniV3Swapper-buyTokensOneStable}"
      },
      "constructor": {
        "details": "Initializes the contract",
        "params": {
          "baseToken_": "The address of the base token of the protocol",
          "curveAaveStaker_": "The address of our Curve Aave staker contract",
          "uniswapV3Pool_": "The address of the Uniswap V3 pool contract"
        }
      },
      "exitOneStable(uint256)": {
        "details": "See {IUniV3Swapper-exitOneStable}"
      },
      "sellTokensForAsset(uint256,address)": {
        "details": "See {IUniV3Swapper-sellTokensForAsset}"
      },
      "sellTokensForStable(uint256,uint256,address)": {
        "details": "See {IUniV3Swapper-sellTokensForStable}"
      },
      "uniswapV3SwapCallback(int256,int256,bytes)": {
        "details": "See {IUniswapV3SwapCallback-uniswapV3SwapCallback} This function is called to the sender after a swap is executed on Uniswap V3. The pool tokens owed for the swap must be payed. The caller of this method must be checked to be a UniswapV3Pool deployed by the canonical UniswapV3Factory. amount0Delta and amount1Delta can both be 0 if no tokens were swapped."
      }
    },
    "stateVariables": {
      "MAX_SQRT_RATIO": {
        "details": "The maximum value that can be returned from {TickMath-getSqrtRatioAtTick} Equivalent to getSqrtRatioAtTick(MAX_TICK)."
      },
      "MIN_SQRT_RATIO": {
        "details": "The minimum value that can be returned from {TickMath-getSqrtRatioAtTick} Equivalent to getSqrtRatioAtTick(MIN_TICK)."
      },
      "assetToken": {
        "details": "The Ultrachess asset token"
      },
      "baseIsToken0": {
        "details": "True if the base token is sorted first in the Uniswap V3 pool, false otherwise"
      },
      "baseToken": {
        "details": "The Ultrachess base token"
      },
      "curveAaveStaker": {
        "details": "The Curve Aave staker contract"
      },
      "uniswapV3Pool": {
        "details": "The Uniswap V3 pool for the token pair"
      }
    },
